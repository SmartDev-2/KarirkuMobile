<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Warna dasar -->
    <solid android:color="#FFFFFFFF" />

    <!-- Garis tepi halus (opsional) -->
